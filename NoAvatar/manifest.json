{
  "manifest_version": 2,

  "name": "NoAvatar",
  "description": "Hides all avatar images on the SDS",
  "version": "1.0",

  "permissions": [
    "tabs", "https://scratch.mit.edu/*"
  ],
  
  "content_scripts": [
    {
      "matches": ["https://scratch.mit.edu/studios/3479432/*"],
      "css": ["style.css"]
    }
  ]
}
